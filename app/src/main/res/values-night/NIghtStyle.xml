<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="myStyleButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_button</item>
        <item name="android:layout_marginTop">@dimen/step</item>
        <item name="android:layout_weight">1</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="cornerRadius">@dimen/radius</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
    </style>
    <style name="myTextStyle">
        <item name="android:paddingBottom">21sp</item>
        <item name="android:paddingTop">21sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">start|center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:drawableTint">@color/black</item>
    </style>
    <style name="myImageStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_marginTop">@dimen/Top</item>
    </style>
    <style name="SettingTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableTint">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/text_size</item>

    </style>
    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawablePadding">@dimen/drawablePadding_9dp</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:hint">@string/Search</item>
        <item name="android:inputType">text</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/black</item>
        <item name="android:drawableTint">@color/black</item>
        <item name="android:paddingLeft">@dimen/paddingLeft_13dp</item>
        <item name="android:src">@drawable/vectorxblack</item>

    </style>
    <style name="ImageClear">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center|end</item>
        <item name="android:paddingRight">@dimen/padingRight_16dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:src">@drawable/vectorxblack</item>
    </style>
    <style name="mySettingTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/layoy_height_61</item>
        <item name="android:gravity">center|start</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="drawableTint">@color/grey</item>
    </style>
    <style name="trackNameStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="artistTrackStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxEms">15</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingRight">@dimen/text_size_11sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="timeTrackStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:paddingLeft">@dimen/pading_left_5dp</item>
        <item name="android:paddingRight">@dimen/text_size_11sp</item>
    </style>
    <style name="imageFrontArrowStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="tint">@color/white</item>
    </style>

</resources>