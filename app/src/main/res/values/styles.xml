<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="myStyleButton" parent="Widget.MaterialComponents.Button.UnelevatedButton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/height_button</item>
        <item name="android:layout_marginTop">@dimen/step</item>
        <item name="android:layout_weight">1</item>
        <item name="android:backgroundTint">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size</item>
        <item name="cornerRadius">@dimen/radius</item>
        <item name="iconGravity">textStart</item>
        <item name="iconTint">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>

    </style>
    <style name="myTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:drawableTint">@color/white</item>
    </style>
    <style name="myImageStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">end</item>
        <item name="android:layout_marginTop">@dimen/Top</item>
    </style>
    <style name="SettingTextStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawableTint">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size</item>

    </style>
    <style name="EditTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:drawablePadding">@dimen/drawablePadding_9dp</item>
        <item name="android:backgroundTint">@color/BackGroundSerch</item>
        <item name="android:hint">@string/Search</item>
        <item name="android:inputType">text</item>
        <item name="android:lines">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textColorHint">@color/grey</item>
        <item name="android:paddingLeft">@dimen/paddingLeft_13dp</item>

    </style>
    <style name="ImageClear">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center|end</item>
        <item name="android:paddingRight">@dimen/padingRight_16dp</item>
        <item name="android:visibility">gone</item>
        <item name="android:src">@drawable/vectorx</item>
    </style>
    <style name="mySettingTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/layoy_height_61</item>
        <item name="android:gravity">center|start</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="drawableTint">@color/grey</item>
    </style>
    <style name="trackNameStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/textSize</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="artistTrackStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxEms">11</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:paddingRight">@dimen/pading_right_5dp</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
    </style>
    <style name="timeTrackStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/grey</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:paddingLeft">@dimen/pading_left_5dp</item>
        <item name="android:paddingRight">@dimen/text_size_11sp</item>
    </style>
    <style name="imageFrontArrowStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="tint">@color/grey</item>
    </style>
    <style name="ImageError">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">@dimen/margin_bottom_140dp</item>
        <item name="android:src">@drawable/light_mode</item>
        <item name="android:visibility">invisible</item>
    </style>
    <style name="ImageErrorNoInternet">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginBottom">@dimen/margin_bottom_140dp</item>
        <item name="android:src">@drawable/light_mode_no_internet</item>
        <item name="android:visibility">invisible</item>
    </style>
    <style name="textError">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textSize">@dimen/text_size_19sp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_marginTop">@dimen/margin_top_40dp</item>
    </style>
    <style name="buttorRefresh">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:layout_marginTop">@dimen/margin_top_100dp</item>
        <item name="android:padding">@dimen/pading_10dp</item>
        <item name="android:text">@string/refresh</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:visibility">invisible</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="cornerRadius">@dimen/radius_20</item>
    </style>
    <style name="textSearcherd">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size</item>
    </style>
    <style name="buttonClearHistory">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:backgroundTint">@color/black</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/ys_display_medium</item>
        <item name="cornerRadius">@dimen/radius_20</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textStyle">bold</item>

    </style>
    <style name="titlesTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/grey_text</item>
        <item name="android:textSize">@dimen/text_size_13sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:layout_marginTop">@dimen/margin_top_17dp</item>
    </style>
    <style name="titlesAttributeTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">@dimen/text_size_13sp</item>
        <item name="android:fontFamily">@font/ys_display_regular</item>
        <item name="android:layout_marginTop">@dimen/margin_top_17dp</item>
    </style>
    <style name="floatingButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fabCustomSize">@dimen/fab_custom_size_51dp</item>
        <item name="android:layout_marginTop">@dimen/margin_top_54dp</item>
        <item name="backgroundTint">@color/floating_button_grey</item>
        <item name="tint">@color/white</item>
        <item name="android:foregroundGravity">center</item>
    </style>
    <style name="play_button">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fabCustomSize">@dimen/fab_custom_size_84dp</item>
        <item name="backgroundTint">@color/black</item>
        <item name="tint">@color/white</item>
        <item name="android:layout_marginTop">@dimen/margin_top_38dp</item>
        <item name="maxImageSize">@dimen/max_image_size_60dp</item>
    </style>
    <style name="playerTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="arrowBackStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/arrow</item>
        <item name="android:layout_marginTop">@dimen/margin_top_20dp</item>
    </style>
</resources>